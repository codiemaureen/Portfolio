mixin cert()
        .projects
            .project
                .project__header
                    h2 NodeJS
                .project__links
                    .gitlink: a(href="/images/NodeCert.jpg") 
                        != h.icon('github')
                    .gitlink: a(href="https://nicole-wedding.herokuapp.com/") 
                        != h.icon('laptop')
                .project__image: a(href="/images/NodeCert.jpg")    
                    img(src="/images/NodeCert.jpg", alt="projects")
            .project
                .project__header
                    h2 ES6
                .project__links
                    .gitlink: a(href="https://github.com/codiemaureen/Nicole_Wedding_Page") 
                        != h.icon('github')
                    .gitlink: a(href="https://nicole-wedding.herokuapp.com/") 
                        != h.icon('laptop')
                .project__image: a(href="/images/ES6Cert.jpg")    
                    img(src="/images/ES6Cert.jpg", alt="projects")
            .project
                .project__header
                    h2 JavaScript
                .project__links
                    .gitlink: a(href="https://github.com/codiemaureen/Nicole_Wedding_Page") 
                        != h.icon('github')
                    .gitlink: a(href="https://nicole-wedding.herokuapp.com/") 
                        != h.icon('laptop')
                .project__image: a(href="/images/JavaScriptCert.jpg")    
                    img(src="/images/JavaScriptCert.jpg", alt="projects")
